//                             وسيلة لمشاركة المعلومات
// 		                            عنوان وقيمة
//   Curly brackets   {} 	    key   and value 	{integer  , string , boolean , object}
//   square brackets  []	    index and value     {integer  , string , boolean , object}
// 		                              البداية

// {

//     "Name"   :  "abdullah"
    
//     ,

//     "age"    :24


// }


[

    {

        "Name"   :  "abdullah"
        
        ,
    
        "age"    :24
    
    
    }

    ,

    24

    
]

