#include<iostream>
using namespace std;
void print(int a,double b,char c);

void main(){
	cout << "Function Data type of parameter" << endl;

	print(10, 9.5, 'd');

	system("pause");
}


void print(int a, double b, char c){
	cout << "int    a is " << a << endl;
	cout << "double b is " << b << endl;
	cout << "char   c is " << c << endl;
}


//تكوين الداله 

//Data-type function-name ( parameter )
