#include<iostream>
using namespace std;
void main(){
	cout << "Example For Variable" << endl;
	int a = 5;

	cout << a << " * 1 = " << a * 1 << endl;
	cout << a << " * 2 = " << a * 2 << endl;
	cout << a << " * 3 = " << a * 3 << endl;
	cout << a << " * 4 = " << a * 4 << endl;
	cout << a << " * 5 = " << a * 5 << endl;
	
	system("pause");	
}

