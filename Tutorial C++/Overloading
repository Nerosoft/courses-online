#include<iostream>
using namespace std;
void login(char username[]);
void login(char username[], char password[]);


void main(){
	cout << "Function Overloading" << endl;
	login("ahmed");
	login("ahmed", "123");
	/*
	الزتونه انك احتفظط بأسم الداله المخصصه لتسجيل الدخول
	بحيث انك تنادي علي الداله وتمرر عدد البراميتر 
	بحيث لو عملية تسجيل الدخول معقده تمرر للداله اسم المستخدم وكلمة السر
	اما اذا كنت عملية تسجيل الدخول ميسره تمرر اسم المستخدم
	*/
	// بس متنساش انك بتستدعي نفس اسم الداله


	system("pause");
}


void login(char username[]){
	cout << "is correct " << endl;
}

void login(char username[], char password[]){
	cout << "is correct " << endl;
}

//تكوين الداله 

//Data-type function-name ( parameter )
