#include<iostream>
using namespace std;
void display(int a );
void display(double a);


void main(){
	cout << "Function polymorphism" << endl;
	display(10);
	display(5.2);
	//الزتونه انك احتفظط بأسم الداله المخصصه للطباعه بحيث انك تنادي علي الداله وتمرر اي نوع من البراميتر 
	// بس متنساش انك بتستدعي نفس اسم الداله
	

	system("pause");
}


void display(int a){
	cout << "int " << a<<endl;
}

void display(double a){
	cout << "double " << a << endl;
}

//تكوين الداله 

//Data-type function-name ( parameter )
