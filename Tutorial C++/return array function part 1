#include<iostream>
using namespace std;
void f1(int a[]);


void main(){
	cout << "return array function part 1" << endl;

	int a[3] = { 1, 2, 3 };
	
	 f1(a);

	
	system("pause");
}

void f1(int a[]){

	int temp[3];

	for (int i = 0, ii = 2; ii >= 0; i++, ii--)

		 temp[i] = a[ii];

	cout << temp[0] << endl << temp[1] << endl << temp[2] << endl<<endl ;
	
}

//تكوين الداله 

